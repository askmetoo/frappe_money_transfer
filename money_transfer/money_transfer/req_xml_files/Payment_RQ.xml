<?xml version="1.0" encoding="utf-8"?>
<FPEnvelope xmlns:header="urn:iso:std:iso:20022:tech:xsd:head.001.001.01" xmlns:document="urn:iso:std:iso:20022:tech:xsd:pacs.008.001.04" xmlns="urn:iso:std:iso:20022:tech:xsd:payment_request">
  <header:AppHdr>
    <header:Fr>
      <header:FIId>
        <header:FinInstnId>
          <header:Othr>
            <header:Id>{header_fr}</header:Id>
          </header:Othr>
        </header:FinInstnId>
      </header:FIId>
    </header:Fr>
    <header:To>
      <header:FIId>
        <header:FinInstnId>
          <header:Othr>
            <header:Id>{header_to}</header:Id>
          </header:Othr>
        </header:FinInstnId>
      </header:FIId>
    </header:To>
    <header:BizMsgIdr>{header_biz_msg}</header:BizMsgIdr>
    <header:MsgDefIdr>{header_msg_def}</header:MsgDefIdr>
    <header:CreDt>{header_cre_dt}</header:CreDt>
  </header:AppHdr>
  <document:Document>
    <document:FIToFICstmrCdtTrf>
      <document:GrpHdr>
        <document:MsgId>{document_msg_id}</document:MsgId>
        <document:CreDtTm>{document_cre_dt_tm}</document:CreDtTm>
        <document:NbOfTxs>{nb_of_txs}</document:NbOfTxs>
        <document:SttlmInf>
          <document:SttlmMtd>{sttlm_mtd}</document:SttlmMtd>
          <document:ClrSys>
            <document:Prtry>{clr_sys}</document:Prtry>
          </document:ClrSys>
        </document:SttlmInf>
        <document:PmtTpInf>
          <document:LclInstrm>
            <document:Prtry>{lcl_instrm_prtry}</document:Prtry>
          </document:LclInstrm>
        </document:PmtTpInf>
        <document:InstgAgt>
          <document:FinInstnId>
            <document:Othr>
              <document:Id>{instg_agt_id}</document:Id>
            </document:Othr>
          </document:FinInstnId>
        </document:InstgAgt>
        <document:InstdAgt>
          <document:FinInstnId>
            <document:Othr>
              <document:Id>{instd_agt_id}</document:Id>
            </document:Othr>
          </document:FinInstnId>
        </document:InstdAgt>
      </document:GrpHdr>
      <document:CdtTrfTxInf>
        <document:PmtId>
          <document:EndToEndId>{end_to_end_id}</document:EndToEndId>
          <document:TxId>{tx_id}</document:TxId>
        </document:PmtId>
        <document:IntrBkSttlmAmt Ccy="{intr_bksttlm_amt_ccy}">{intr_bksttlm_amt}</document:IntrBkSttlmAmt>
        <document:AccptncDtTm>{accptnc_dt_tm}</document:AccptncDtTm>
        <document:InstdAmt Ccy="{instd_amt_ccy}">{instd_amt}</document:InstdAmt>
        <document:ChrgBr>{chrg_br}</document:ChrgBr>
        <document:UltmtDbtr>
          <document:Nm>{ultmt_dbtr_nm}</document:Nm>
          <document:Id>
            <document:OrgId>
              <document:Othr>
                <document:Id>{ultmt_dbtr_id}</document:Id>
                <document:SchmeNm>
                  <document:Prtry>{ultmt_dbtr_prtry}</document:Prtry>
                </document:SchmeNm>
              </document:Othr>
            </document:OrgId>
          </document:Id>
        </document:UltmtDbtr>
        <document:Dbtr>
          <document:Nm>{dbtr_nm}</document:Nm>
          <document:PstlAdr>
            <document:AdrLine>{dbtr_adr_line}</document:AdrLine>
          </document:PstlAdr>
          <document:CtctDtls>
            <document:Othr>{dbtr_othr}</document:Othr>
          </document:CtctDtls>
        </document:Dbtr>
        <document:DbtrAcct>
          <document:Id>
            <document:Othr>
              <document:Id>{dbtr_acct_id}</document:Id>
              <document:SchmeNm>
                <document:Prtry>{dbtr_acct_prtry}</document:Prtry>
              </document:SchmeNm>
              <document:Issr>{dbtr_acct_issr}</document:Issr>
            </document:Othr>
          </document:Id>
        </document:DbtrAcct>
        <document:DbtrAgt>
          <document:FinInstnId>
            <document:PstlAdr>
              <document:BldgNb>{dbtr_agt_bldg_nb}</document:BldgNb>
            </document:PstlAdr>
            <document:Othr>
              <document:Id>{dbtr_agt_id}</document:Id>
              <document:Issr>{dbtr_agt_issr}</document:Issr>
            </document:Othr>
          </document:FinInstnId>
          <document:BrnchId>
            <document:Nm>{dbtr_agt_nm}</document:Nm>
          </document:BrnchId>
        </document:DbtrAgt>
        <document:CdtrAgt>
          <document:FinInstnId>
            <document:Othr>
              <document:Id>{cdtr_agt_id}</document:Id>
            </document:Othr>
          </document:FinInstnId>
        </document:CdtrAgt>
        <document:Cdtr>
          <document:Nm>{cdtr_nm}</document:Nm>
        </document:Cdtr>
        <document:CdtrAcct>
          <document:Id>
            <document:Othr>
              <document:Id>{cdtr_acct_id}</document:Id>
              <document:SchmeNm>
                <document:Prtry>{cdtr_prtry}</document:Prtry>
              </document:SchmeNm>
            </document:Othr>
          </document:Id>
        </document:CdtrAcct>
        <document:RmtInf>
          <document:Ustrd>{ustrd}</document:Ustrd>
        </document:RmtInf>
      </document:CdtTrfTxInf>
    </document:FIToFICstmrCdtTrf>
  </document:Document>
</FPEnvelope>